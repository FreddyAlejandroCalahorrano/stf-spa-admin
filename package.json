{
  "name": "stf-spa-admin",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local",
    "start:dev": "ng serve -c dev",
    "start:cert": "ng serve -c cert",
    "start:prod": "ng serve -c production",
    "build": "ng build -c local",
    "build:production": "ng build -c production --base-href /admin/ --deploy-url /admin/ && mv dist/admin/index.html ./dist",
    "build:cert": "ng build -c cert",
    "build:development": "ng build -c dev --base-href /admin/ --deploy-url /admin/ && mv dist/admin/index.html ./dist",
    "build:staging": "ng build -c cert --base-href /admin/ --deploy-url /admin/ && mv dist/admin/index.html ./dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "test:deploy": "ng test --watch=false --coverage",
    "test:watch": "ng test --watch",
    "coverage": "ng test --watch=false --coverage",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@pichincha/angular-sdk": "1.0.17",
    "@pichincha/bb-commons": "1.0.15",
    "@pichincha/typescript-sdk": "1.0.23",
    "file-saver": "^2.0.2",
    "ngx-build-plus": "^12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "xng-breadcrumb": "^6.8.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.0",
    "@faker-js/faker": "^7.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "jest": "^27.0.0",
    "typescript": "~4.3.5"
  }
}
